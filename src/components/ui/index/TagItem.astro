---
import type { IterableRenderer, PostIndex } from "@/types";
import { Icon } from "astro-icon/components";
import Item from "./Item.astro";

interface Props extends IterableRenderer<PostIndex> {}

const { id, index, item } = Astro.props;
---

<Item id={id} index={index} item={item}>
  <Icon name="lucide:tag" slot="icon" />
</Item>
